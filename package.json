{
  "name": "git-rebase-all-cli",
  "version": "1.1.0",
  "description": "여러 PR 브랜치를 main에 rebase하여 충돌을 자동으로 해결하는 Git 도구",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "git-rebase-all": "dist/cli.js",
    "gra": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && node scripts/add-shebang.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepublish": "npm run build",
    "publish:check": "npm pack --dry-run",
    "publish:pnpm": "pnpm run build && pnpm publish",
    "publish:public": "pnpm run build && pnpm publish --access public",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "git",
    "rebase",
    "automation",
    "cli",
    "branch",
    "conflict",
    "pr"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whizzkid1452/git-rebase-all-cli.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1",
    "chalk": "^5.3.0",
    "execa": "^9.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
